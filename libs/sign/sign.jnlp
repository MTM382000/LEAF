<?xml version="1.0" encoding="UTF-8"?>
<jnlp spec="1.5.0+" codebase="http://localhost/libs/sign" href="sign.jnlp">

    <information>
        <title>Websocket Digital Signature</title>
    </information>
    <security>
        <all-permissions/>
    </security>
    <resources arch="amd64">
        <j2se version="1.7+" java-vm-args="-d64 -noverify"/>
        <jar href="Sign.jar" />
    </resources>

    <application-desc main-class="leaf.Application"/>
    <update check="background"/>
</jnlp>